<Window x:Class="EntityFramework_HomeTasks_5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EntityFramework_HomeTasks_5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="ProductsMenuButton" Content="Manage Products" HorizontalAlignment="Center" Width="376" FontSize="18" Click="ProductsMenuButton_Click" VerticalAlignment="Top" Height="55" Margin="0,56,0,0"/>
        <Button x:Name="CategoriesMenuButton" Content="Manage Categories" HorizontalAlignment="Center" Width="376" FontSize="18" VerticalAlignment="Top" Height="55" Margin="0,144,0,0" Click="CategoriesMenuButton_Click"/>
        <Button x:Name="KeyWordsMenuButton" Content="Manage KeyWords" HorizontalAlignment="Center" Width="376" FontSize="18" VerticalAlignment="Top" Height="55" Margin="0,228,0,0" Click="KeyWordsMenuButton_Click"/>
        <Button x:Name="BackMenuButton" Content="BackUp" HorizontalAlignment="Center" Width="188" FontSize="18" VerticalAlignment="Top" Height="47" Margin="0,331,0,0" Click="BackMenuButton_Click"/>

    </Grid>
</Window>
